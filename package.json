{
  "name": "@muhlba91/muehlbachler-shared-services",
  "version": "0.0.0",
  "description": "muehlbachler: Shared Services",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "repository": "https://github.com/muhlba91/muehlbachler-shared-services",
  "license": "MIT",
  "keywords": [
    "pulumi",
    "muehlbachler",
    "shared-services"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "fix": "run-s fix:prettier fix:lint",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint --fix",
    "lint": "eslint",
    "watch:build": "tsc -p tsconfig.json -w",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --json build/docs/typedoc.json"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.5.1",
  "dependencies": {
    "@pulumi/aws": "^6.2.0",
    "@pulumi/mongodbatlas": "^3.14.1",
    "@pulumi/pulumi": "^3.69.0",
    "@pulumi/random": "^4.14.0",
    "@pulumi/vault": "^6.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@types/node": "^22.7.6",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^7.0.0",
    "eslint-plugin-import": "^2.29.1",
    "npm-run-all2": "^7.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.1"
  },
  "prettier": {
    "singleQuote": true
  }
}
